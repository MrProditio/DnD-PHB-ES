{
  "id": "phb24-and-modules-es",
  "name": "PHB24-and-Modules-ES",
  "title": "PHB24 y Módulos en Español",
  "description": "Traducción al español del Player’s Handbook 2024 para Foundry VTT y de módulos compatibles como DFreds Convenient Effects",
  "version": "1.0.2",
  "author": "MrProditio",
  "compatibility": {
    "minimum": "13",
    "verified": "13",
    "maximum": "13"
  },
  "system": "dnd5e",
  "systemVersions": ["5.1.8"],
  "esmodules": ["babele-register.js"],
  "url": "https://github.com/MrProditio/DnD-PHB-ES",
  "manifest": "https://raw.githubusercontent.com/MrProditio/DnD-PHB-ES/main/module.json",
  "download": "https://github.com/MrProditio/DnD-PHB-ES/archive/refs/heads/main.zip",
  "license": "MIT",
  "languages": [
    {
      "lang": "es",
      "name": "Español",
      "path": "translations/PHB24/es"
    },
    {
      "lang": "es-ce",
      "name": "Español (Convenient Effects)",
      "path": "translations/CE/es"
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "babele",
        "type": "module"
      }
    ]
  }
}


